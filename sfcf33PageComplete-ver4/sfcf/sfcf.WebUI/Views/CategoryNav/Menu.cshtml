@using sfcf.Domain.Entities;
@model IEnumerable<Category>


@foreach (var Cat in Model)
{
    <li>
        @Html.RouteLink(Cat.Name, new
            {
                controller = "Voting",
                action = "List",
                categoryId = Cat.ID,
                page = 1
            })
    </li>
}

